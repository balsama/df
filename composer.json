{
    "name": "drupal/df",
    "description": "A framework for demoing enterprise-ready Drupal solutions.",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "composer", "url": "https://packagist.drupal-composer.org" },
        {
            "type": "package",
            "package": {
                "name": "mjolnic/fontawesome-iconpicker",
                "version": "1.2.0",
                "source": {
                    "url": "https://github.com/mjolnic/fontawesome-iconpicker",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.5.0",
                "source": {
                    "url": "https://github.com/kenwheeler/slick",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "loopindex/ckeditor-track-changes",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/loopindex/ckeditor-track-changes",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "8.2.*-dev",
        "drupal/address": "8.1.x-dev",
        "drupal/addtoany": "8.1.3",
        "drupal/admin_toolbar": "8.1.x-dev",
        "drupal/better_formats": "8.1.x-dev",
        "drupal/block_class": "8.1.x-dev",
        "drupal/dropzonejs": "8.1.0-alpha1",
        "drupal/composer_manager": "8.1.x-dev",
        "drupal/config_devel": "8.1.x-dev",
        "drupal/config_update": "8.1.1",
        "drupal/config_sync": "8.1.x-dev",
        "drupal/content_browser": "8.1.x-dev",
        "drupal/crop": "8.1.0-alpha2",
        "drupal/ctools": "8.3.0-alpha25",
        "drupal/embed": "8.1.0-rc2",
        "drupal/entity_browser": "8.1.x-dev",
        "drupal/entity_embed": "8.1.0-alpha1",
        "drupal/features": "8.3.0-beta3",
        "drupal/field_group": "8.1.0-rc4",
        "drupal/file_browser": "8.1.x-dev",
        "drupal/file_entity": "8.2.x-dev",
        "drupal/geocoder": "8.2.0-alpha3",
        "drupal/geofield": "8.1.x-dev",
        "drupal/geophp": "8.1.x-dev",
        "drupal/geolocation": "8.1.x-dev",
        "drupal/google_analytics": "8.2.0",
        "drupal/image_widget_crop": "8.1.0",
        "drupal/import": "8.2.x-dev",
        "drupal/inline_entity_form": "8.1.x-dev",
        "drupal/layout_plugin": "8.1.0-alpha22",
        "drupal/leaflet": "8.1.x-dev",
        "drupal/libraries": "8.3.x-dev",
        "drupal/lite": "8.1.x-dev",
        "drupal/moderate_mmenu": "8.1.x-dev",
        "drupal/migrate_plus": "8.2.x-dev",
        "drupal/migrate_source_csv": "8.2.x-dev",
        "drupal/migrate_tools": "8.2.0-beta1",
        "drupal/oauth": "8.1.x-dev",
        "drupal/page_manager": "8.1.0-alpha23",
        "drupal/panels": "8.3.x-dev",
        "drupal/panelizer": "8.3.0-alpha2",
        "drupal/pathauto": "8.1.0-alpha3",
        "drupal/refreshless": "8.1.0-alpha3",
        "drupal/restui": "8.1.x-dev",
        "drupal/scenarios": "8.2.x-dev",
        "drupal/scheduler": "8.1.0-alpha1",
        "drupal/token": "8.1.0-alpha2",
        "drupal/url_embed": "8.1.x-dev",
        "drupal/views_infinite_scroll": "8.1.0",
        "drupal/workbench_moderation": "8.1.0",
        "drupal/adminimal_theme": "8.1.1",
        "drupal/zurb_foundation": "8.6.x-dev",
        "enyo/dropzone": "4.2.0",
        "mjolnic/fontawesome-iconpicker": "1.2.0",
        "desandro/masonry": "dev-master",
        "desandro/imagesloaded": "dev-master",
        "loopindex/ckeditor-track-changes": "dev-master#a7df5c6b685385713e8aaf2df3f44c82e3bf823a",
        "kenwheeler/slick": "1.5.0"
    }, 
    "scripts": {
        "post-install-cmd": [
            "mkdir profiles/df/libraries && cd profiles/df/libraries && ln -s ../../../vendor/loopindex/ckeditor-track-changes lite"
        ]
    },
    "extra": {
        "installer-paths": {
            "core": ["type:drupal-core"],
            "profiles/df/modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/df/themes/contrib/{$name}": ["type:drupal-theme"]
        },
        "patches": {
            "drupal/drupal": {
                "Add authentication support for Views | https://www.drupal.org/node/2228141":
                "https://www.drupal.org/files/issues/add_authentication-2228141-54.patch",
                "ConnectionNotDefinedException thrown... | https://www.drupal.org/node/2703669":
                "https://www.drupal.org/files/issues/node-migration-traits-database-exception.patch",
                "Quickedit cant edit images | https://www.drupal.org/node/2635712":
                "https://www.drupal.org/files/issues/2635712-14.patch",
                "Can we test RefreshLess with simplytest.me? | https://www.drupal.org/node/2695717":
                "https://www.drupal.org/files/issues/refreshless-alpha3-core-patch-2695717-7.patch"
            },
            "drupal/better_formats": {
                "Fix field validation. | http://drupal.org/node/2709313":
                "https://www.drupal.org/files/issues/bef_field_is_not-2709313-2.patch"
            },
            "drupal/block_class": {
                "Add support for page manager. | http://drupal.org/node/2509142":
                "https://www.drupal.org/files/issues/add-page-manager-support-2509142-2.patch"
            },
            "drupal/lite": {
                "Lite uses the wrong path | https://www.drupal.org/node/2718189":
                "https://www.drupal.org/files/issues/2718189-3.patch"
            },
            "drupal/page_manager": {
                "Rename the core 'Page with blocks' admin_label... | https://www.drupal.org/node/2503947":
                "https://www.drupal.org/files/issues/page_manager-asort-admin-label-etc-0.patch"
            },
            "drupal/panels": {
                "Allow other modules to disable the IPE based on custom logic | https://www.drupal.org/node/2667754":
                "https://www.drupal.org/files/issues/2667754-3.patch",
                "Editing layouts via IPE affects other users... | https://www.drupal.org/node/2701433":
                "https://www.drupal.org/files/issues/bandaid.patch",
                "IPE's cancel button is easy to mistake for a button... | https://www.drupal.org/node/2636490":
                "https://www.drupal.org/files/issues/panels-ipe-esc-focus-fixes-2636490-9.patch"
            },
            "drupal/panelizer": {
                "Enter drupal/panelizer patch #2700597 description here":
                "https://www.drupal.org/files/issues/panelizer-ipe-url-root-handling.patch"
            },
            "drupal/url_embed": {
                "Explicitly set the Panels IPE URL root when saving in Panelizer | https://www.drupal.org/node/2700597":
                "https://www.drupal.org/files/issues/2546204-6.patch"
            },
            "drupal/workbench_moderation": {
                "Provide logical access control for when IPE... | https://www.drupal.org/node/2668006":
                "https://www.drupal.org/files/issues/2668006-2.patch",
                "Replace the workbench moderation form with a block | https://www.drupal.org/node/2685163":
                "https://www.drupal.org/files/issues/2685163-19.patch"
            },
            "drupal/adminimal_theme": {
                "Fix styling so that admin_toolbar can be used | https://www.drupal.org/node/2680689":
                "https://www.drupal.org/files/issues/adminimal_theme-fix-styles-for-admin-toolbar-2680689-7.patch"
            },
            "loopindex/ckeditor-track-changes": {
                "CKLite interferes with content insertion | http://drupal.org/node/2482879":
                "https://www.drupal.org/files/issues/cklite-content-insertion-2482879-4.patch"
            }
        }
    }
}
